# ===================== MiniQuantDesk — Example env =====================
# Copy this file to `env` and fill in your own keys locally.
# Do not commit your filled env — commit only this template.

# ===== 1) Core bot =====
MODE=BACKTEST
BACKTEST_INTERVAL=1Day
BACKTEST_LOOKBACK_DAYS=120
BACKTEST_PASSES=3
BACKTEST_TARGET_TRADES_PER_SYMBOL=100
BACKTEST_MAX_PASSES=50
SYMBOLS=AAPL,MSFT
BACKTEST_APPEND_ONLY=true

# ===== 2) Strategy =====
STRATEGY=abcd
TAKE_PROFIT_PCT=0.05
STOP_LOSS_PCT=0.02
RISK_PER_TRADE=0.01
SMA_FAST=5
SMA_SLOW=20
RSI_PERIOD=14
RSI_MIN_BUY=45
RSI_MAX_SELL=70

# ===== 3) Alpaca API =====
# Paper
APCA_API_KEY_ID_PAPER=
APCA_API_SECRET_KEY_PAPER=
APCA_PAPER_BASE_URL=https://paper-api.alpaca.markets
# Live
APCA_API_KEY_ID=
APCA_API_SECRET_KEY=
APCA_LIVE_BASE_URL=https://api.alpaca.markets

# ===== 4) Logging & Paths =====
LOG_DIR=
DATA_DIR=

# ===== 5) Versioning / ML =====
STRATEGY_VERSION=s1.0
FEATURE_VERSION=f1.0
MODEL_VERSION=none
USE_MODEL=false

# ===== 6) Discord / Webhooks =====
DISCORD_WEBHOOK_BACKTEST=
DISCORD_WEBHOOK_PAPER=
DISCORD_WEBHOOK_LIVE=
DISCORD_WEBHOOK_SCANNER=
DISCORD_WEBHOOK_HEARTBEAT=
DISCORD_BOT_TOKEN=
DISCORD_KILL_PHRASE=
DISCORD_KILL_CHANNEL=

# Kill-switch
KILL_FILE=ml_sidecar/logs/kill.txt
KILL_URL=

# ===== 7) Supervisors =====
MARKET_TZ=America/New_York
MARKET_OPEN=09:30
MARKET_CLOSE=16:00
RUN_PREMARKET=true
RUN_POSTMARKET=false
SUPERVISOR_POLL_SEC=15

BOT_CMD=python -m apps.executor.TradingBot
SIDECAR_CMD=python ml_sidecar/sidecar/logger_service.py
SUP_RESTART_BACKOFF_SEC=5
# ===================== End of example env =====================
